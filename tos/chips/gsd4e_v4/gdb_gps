output SirfBinP__sirfbin_state_prev
printf ",  "
output SirfBinP__sirfbin_state
printf "\n      ptr: (p) %08x,  %08x\n", \
    SirfBinP__sirfbin_ptr_prev, SirfBinP__sirfbin_ptr

printf "\nDriver: "
output gpsc_state
printf "\nevents:\n"
output g_evs
printf "\n\n"

end
